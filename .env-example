# ============================================
# CONFIGURATION SUPABASE - REMPLACER PAR VOS VALEURS
# ============================================

# URL de votre projet Supabase
SUPABASE_URL=https://votre-projet-id.supabase.co

# Clé publique anonyme (anon key)
SUPABASE_KEY=votre-anon-key-ici

# Clé de service (service_role key) - À GARDER SECRET !
SUPABASE_SERVICE_ROLE_KEY=votre-service-role-key-ici

# ============================================
# CONFIGURATION APPLICATION
# ============================================

ENV=development
DEBUG=true
LOG_LEVEL=INFO

# ============================================
# CONFIGURATION STORAGE
# ============================================

STORAGE_BUCKET=datasets
MAX_FILE_SIZE_MB=100

DB_HOST=db.votre-projet-id.supabase.co
DB_PORT=5432
DB_NAME=postgres
DB_USER=postgres
DB_PASSWORD=votre-mot-de-passe-projet